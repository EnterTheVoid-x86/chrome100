{
  "compilerOptions": {
    "outDir": "dist/static",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "strict": true,
    "sourceMap": true,
    "inlineSources": true,
    "jsx": "react",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment"
  },
  "include": ["src/*.ts", "./src/*.tsx"]
}
